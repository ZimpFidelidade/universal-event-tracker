{
  "name": "universal-event-tracker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "lint": "eslint lib test",
    "lint:fix": "eslint lib test --fix",
    "david": "david",
    "test:once": "tape 'test/**/*.js' | tap-spec",
    "cover": "nyc npm run test:once",
    "test": "npm run lint && npm run david && npm run cover",
    "test:watch": "watch 'npm run lint && npm run cover' lib test",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "preversion": "rm -rf ./node_modules/ && npm install && npm test",
    "postversion": "git push && npm publish && git push --tags"
  },
  "nyc": {
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "check-coverage": true,
    "all": true,
    "cache": true,
    "include": ["lib"],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  },
  "pre-commit": [
    "precommit-msg",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZimpFidelidade/universal-event-tracker.git"
  },
  "keywords": [
    "event tracker"
  ],
  "author": "Zimp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZimpFidelidade/universal-event-tracker/issues"
  },
  "homepage": "https://github.com/ZimpFidelidade/universal-event-tracker#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "david": "^8.0.0",
    "eslint": "^3.2.2",
    "nyc": "^7.1.0",
    "pre-commit": "^1.1.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "watch": "^0.19.1"
  },
  "dependencies": {}
}
