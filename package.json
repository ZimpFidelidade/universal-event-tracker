{
  "name": "universal-event-tracker",
  "version": "2.1.4",
  "description": "A universal event tracker for analytcs",
  "main": "dist/index.js",
  "directories": {
    "test": "test",
    "trackers": "trackers"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "lint": "eslint lib trackers test",
    "lint:fix": "eslint lib trackers test --fix",
    "david": "david",
    "test:once": "tape 'test/**/*.js' | tap-spec",
    "cover": "nyc npm run test:once",
    "test": "echo 1",
    "test:watch": "watch 'npm run lint && npm run cover' lib trackers test",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "build": "babel . -d dist --ignore=node_modules,coverage,docs,test,dist",
    "preversion": "npm test && npm run build",
    "postversion": "git push upstream master && npm publish && git push upstream master --tags"
  },
  "nyc": {
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "check-coverage": true,
    "all": true,
    "cache": true,
    "include": [
      "lib",
      "trackers"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  },
  "pre-commit": [
    "precommit-msg",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZimpFidelidade/universal-event-tracker.git"
  },
  "keywords": [
    "event tracker"
  ],
  "author": "Zimp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZimpFidelidade/universal-event-tracker/issues"
  },
  "homepage": "https://github.com/ZimpFidelidade/universal-event-tracker#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "david": "^8.0.0",
    "eslint": "^3.2.2",
    "nyc": "^8.1.0",
    "pre-commit": "^1.1.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "watch": "^0.19.1"
  },
  "dependencies": {
    "intercom.io": "^1.2.1"
  }
}
